{
	"name": "split-snippet-transform",
	"displayName": "Split Snippet Transform",
	"description": "Split a selection and use its part in a snippet like manner",
	"publisher": "EinWesen",
    "repository": {
        "type": "git",
        "url": "https://github.com/EinWesen/vscode-split-snippet-transform"
    },	
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:einwesen.split-snippet-transform.commands.insert_clipboard",
		"onCommand:einwesen.split-snippet-transform.commands.insert_clipboard_with_split",
		"onCommand:einwesen.split-snippet-transform.commands.insert_snippet"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [ 	
			{
				"command": "einwesen.split-snippet-transform.commands.insert_clipboard",
				"title": "Insert Snippet: Clipboard"
			},
			{
				"command": "einwesen.split-snippet-transform.commands.insert_clipboard_with_split",
				"title": "Insert Snippet: Clipboard with SplitTransform"
			},
			{
				"command": "einwesen.split-snippet-transform.commands.insert_snippet",
				"title": "Insert Snippet: ... with SplitTransform"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.1.4",
		"vscode": "^1.1.25",
		"tslint": "^5.8.0",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	}
}
